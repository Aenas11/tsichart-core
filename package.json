{
  "name": "@tsichart/core",
  "version": "1.0.0",
  "description": "Time Series Interactive Charts - A comprehensive JavaScript library for visualizing time series data with interactive charts including line charts, bar charts, pie charts, heatmaps, scatter plots, and more.",
  "keywords": [
    "timeseries",
    "time-series",
    "charts",
    "visualization",
    "interactive",
    "data-viz",
    "d3",
    "typescript",
    "analytics",
    "dashboard",
    "tsichart",
    "line-chart",
    "heatmap"
  ],
  "main": "tsiclient.js",
  "types": "tsiclient.d.ts",
  "module": "tsiclient.js",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "files": [
    "dist",
    "tsiclient.js",
    "tsiclient.d.ts",
    "tsiclient.css",
    "tsiclient.min.css",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Aenas11/tsichart-core"
  },
  "bugs": {
    "url": "https://github.com/Aenas11/tsichart-core/issues"
  },
  "homepage": "https://github.com/Aenas11/tsichart-core#readme",
  "author": "Alex Sysoiev <alexander.sysoev@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "^22.5.1",
    "npm": "^10.8.2"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "npm run clean & npm run rollup & npm run webpack & npm run post-build",
    "clean": "shx rm -rf dist & shx rm -rf build_artifacts",
    "rollup": "rollup --c & shx rm dist/tsiclient.js",
    "webpack": "webpack --config webpack.prod.js",
    "webpack_dev": "webpack --config webpack.dev.js",
    "copy-package": "shx cp package.json dist/",
    "copy-readme": "shx cp README.MD dist/",
    "pack": "cd dist & npm pack & shx mv *.tgz ../build_artifacts",
    "post-build": "npm run copy-package & npm run copy-readme & npm run pack",
    "umd-analyze": "webpack-bundle-analyzer ./build_artifacts/umd_stats.json"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss-url": "^10.1.3",
    "rollup": "^4.52.4",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^6.0.4",
    "sass-loader": "^16.0.5",
    "shx": "^0.4.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.102.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@types/awesomplete": "^1.1.16",
    "@types/d3": "^7.4.3",
    "awesomplete": "^1.1.7",
    "d3": "^7.9.0",
    "d3-interpolate-path": "^2.3.0",
    "d3-voronoi": "^1.1.4",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "sass": "1.93.2",
    "split.js": "^1.6.5"
  }
}
