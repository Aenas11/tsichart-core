{
  "name": "@tsichart/vue",
  "version": "2.0.0",
  "description": "Vue components for TSIChart",
  "keywords": [
    "vue",
    "vue3",
    "tsichart",
    "charts",
    "timeseries",
    "visualization"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aenas11/tsichart-core.git",
    "directory": "packages/vue"
  },
  "author": "Alex Sysoiev <alexander.sysoev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "shx rm -rf dist"
  },
  "peerDependencies": {
    "@tsichart/core": "workspace:*",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^2.4.0",
    "shx": "^0.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3",
    "vue": "^3.3.0"
  }
}
