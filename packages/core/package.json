{
  "name": "tsichart-core",
  "version": "2.1.0",
  "description": "Framework-agnostic time series charting library - Core package",
  "keywords": [
    "timeseries",
    "time-series",
    "charts",
    "visualization",
    "interactive",
    "data-viz",
    "d3",
    "typescript",
    "analytics",
    "dashboard",
    "tsichart",
    "line-chart",
    "heatmap"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.mjs",
      "require": "./dist/components/*.js"
    },
    "./models/*": {
      "types": "./dist/models/*.d.ts",
      "import": "./dist/models/*.mjs",
      "require": "./dist/models/*.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js"
    },
    "./styles": {
      "types": "./src/styles.d.ts",
      "default": "./dist/styles/index.css"
    },
    "./styles/*": {
      "types": "./src/styles.d.ts",
      "default": "./dist/styles/*.css"
    }
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "files": [
    "dist",
    "src/styles.d.ts",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aenas11/tsichart-core.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/Aenas11/tsichart-core/issues"
  },
  "homepage": "https://aenas11.github.io/tsichart-core/",
  "author": "Alex Sysoiev <alexander.sysoev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "dev": "rollup -c --watch",
    "dev:server": "webpack serve --config webpack.dev.js",
    "dev:build": "webpack --config webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "shx rm -rf dist coverage dev-dist"
  },
  "peerDependencies": {
    "d3": "^7.0.0"
  },
  "dependencies": {
    "@types/awesomplete": "^1.1.16",
    "@types/d3": "^7.4.3",
    "awesomplete": "^1.1.7",
    "d3": "^7.9.0",
    "d3-interpolate-path": "^2.3.0",
    "d3-voronoi": "^1.1.4",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "split.js": "^1.6.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.7.2",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "postcss-url": "^10.1.3",
    "rollup": "^4.52.4",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^6.0.4",
    "sass": "1.93.2",
    "sass-loader": "^16.0.5",
    "shx": "^0.4.0",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
